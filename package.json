{
  "name": "test",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm.mjs ./index.ts",
    "test:all": "ts-mocha -n loader=ts-node/esm.mjs --serial --spec ./test/**/*.spec.ts",
    "test:api": "ts-mocha -n loader=ts-node/esm.mjs --serial --spec ./routes/**/*.spec.ts",
    "test:net": "ts-mocha -n loader=ts-node/esm.mjs --serial --spec ./test/net/*.spec.ts"
  },
  "imports": {
    "#lib/*": "./src/lib/*.js",
    "#routes/*": "./routes/*.js",
    "#classes/*": "./src/classes/*.js",
    "#constants": "./src/constants/index.js",
    "#secrets": "./src/constants/secrets.js",
    "#db": "./src/db/index.js",
    "#vm": "./src/lib/vm/index.js",
    "#routines": "./src/lib/vm/routines.js"
  },
  "dependencies": {
    "async-mutex": "^0.3.2",
    "express": "^4.18.1",
    "ipfs-http-client": "^57.0.3",
    "level-rocksdb": "^5.0.0",
    "secp256k1": "^4.0.3"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/chai": "^4.3.1",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.6",
    "@types/secp256k1": "^4.0.3",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "ts-loader": "^9.3.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0"
  }
}
